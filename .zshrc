# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

alias hn='scutil --set HostName "localhost"'

source "$HOME/.sdkman/bin/sdkman-init.sh"

